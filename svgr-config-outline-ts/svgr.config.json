{
  "icon": "1.5rem",
  "typescript": true,
  "dimensions": true,
  "replaceAttrValues": {
    "black": "currentColor"
  },
  "expandProps": "end",
  "svgProps": {
    "stroke": "currentColor"
  },
  "svgoConfig": {
    "plugins": [
      {
        "name": "removeAttrs",
        "params": {
          "attrs": "(fill|stroke)"
        }
      },
      {
        "name": "addAttributesToSVGElement",
        "params": {
          "attributes": [
            {
              "width": "1.5rem"
            },
            {
              "height": "1.5rem"
            },
            {
              "fill": "none"
            }
          ]
        }
      },
      {
        "name": "mergePaths",
        "params": {
          "force": true
        }
      }
    ]
  }
}
